{
  "name": "@instagramjs/openapi-facebook-graph",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "lint": "eslint",
    "typecheck": "tsc --noEmit",
    "dev": "pnpm build --watch",
    "build": "tsup src/index.ts --format cjs,esm --dts --clean --no-splitting",
    "gen:flows2openapi": "tsx ./scripts/flows2openapi.ts",
    "gen:openapi": "openapi-typescript ./schema.yaml -o ./src/schema.ts"
  },
  "devDependencies": {
    "@instagramjs/eslint-config": "workspace:*",
    "@instagramjs/flows2openapi": "workspace:*",
    "@instagramjs/tsconfig": "workspace:*",
    "@mongodb-js/zstd": "^2.0.1",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.15.29",
    "eslint": "^9.28.0",
    "openapi-typescript": "^7.8.0",
    "tsup": "^8.5.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.0",
    "yaml": "^2.8.0",
    "zod": "^3.25.42"
  },
  "dependencies": {
    "openapi-fetch": "^0.14.0"
  }
}
