{
  "name": "@instagramjs/instagram.js",
  "version": "0.0.3",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "lint": "eslint",
    "typecheck": "tsc --noEmit",
    "dev": "pnpm build --watch",
    "build": "tsup src/index.ts --format cjs,esm --dts --clean --no-splitting"
  },
  "devDependencies": {
    "@instagramjs/eslint-config": "workspace:*",
    "@instagramjs/tsconfig": "workspace:*",
    "@swc/core": "^1.11.29",
    "@t3-oss/env-core": "^0.10.1",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.15.29",
    "dotenv": "^16.5.0",
    "eslint": "^9.28.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.0"
  },
  "dependencies": {
    "@discordjs/collection": "^2.1.1",
    "@instagramjs/api": "workspace:*",
    "@instagramjs/api-types": "workspace:*",
    "@instagramjs/realtime": "workspace:*",
    "axios": "^1.9.0",
    "eventemitter3": "^5.0.1",
    "pino": "^9.7.0",
    "tsx": "^4.19.4",
    "zod": "^3.25.42"
  }
}
