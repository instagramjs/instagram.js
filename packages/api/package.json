{
  "name": "@instagramjs/api",
  "version": "0.0.1",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit",
    "dev": "pnpm build --watch",
    "build": "tsup src/index.ts --format cjs,esm --dts --clean --no-splitting"
  },
  "devDependencies": {
    "@instagramjs/eslint-config": "workspace:*",
    "@instagramjs/prettier-config": "workspace:*",
    "@instagramjs/tsconfig": "workspace:*",
    "@swc/core": "catalog:",
    "@t3-oss/env-core": "catalog:",
    "@types/chance": "1.1.6",
    "@types/eslint": "catalog:",
    "@types/node": "catalog:",
    "dotenv": "catalog:",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:",
    "typescript-eslint": "catalog:"
  },
  "dependencies": {
    "@instagramjs/api-types": "workspace:*",
    "axios": "1.7.7",
    "axios-cookiejar-support": "5.0.3",
    "chance": "1.1.12",
    "eventemitter3": "5.0.1",
    "pino": "catalog:",
    "qs": "6.13.0",
    "tough-cookie": "5.0.0",
    "tsx": "catalog:",
    "zod": "catalog:"
  },
  "prettier": "@instagramjs/prettier-config"
}